# -*- coding: utf-8 -*-
# UTF-8 encoding when using korean
import sys
input = sys.stdin.readline
user_input = input()
N = int(user_input)

temp = []
while N:
	temp.append(N%2)
	N = N//2

temp = temp[::-1]
temp = map(str, temp)
print(''.join(temp))


